{
    "name": "branogoga/todo-php",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "authors": [{
        "name": "Brano Goga",
        "email": "brano.goga@centrum.sk"
    }],
    "readme": "./README.md",
    "repositories": [{
        "type": "path",
        "url": "../../../"
    }],
    "require": {
        "php": ">=8",
        "nette/nette": "~3",
        "dibi/dibi": "~4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "phpstan/phpstan": "~1",
        "phpstan/phpstan-dibi": "~1",
        "phpstan/extension-installer": "~1.0",
        "phpstan/phpstan-nette": "~1"
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "autoload": {
        "classmap": ["app"]
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
