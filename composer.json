{
    "name": "branogoga/todo-php",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "authors": [{
        "name": "Brano Goga",
        "email": "brano.goga@centrum.sk"
    }],
    "readme": "./README.md",
    "repositories": [{
        "type": "path",
        "url": "../../../"
    }],
    "require": {
        "php": ">=8",
        "nette/nette": "~3",
        "dibi/dibi": "~4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "phpstan/phpstan": "~0.12",
        "phpstan/phpstan-dibi": "~0.12",
        "phpstan/extension-installer": "~1.0",
        "phpstan/phpstan-nette": "~0.12"
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "8.0"
        }
    },
    "autoload": {
        "classmap": ["www/app"]
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
